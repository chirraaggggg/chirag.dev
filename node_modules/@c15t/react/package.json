{
  "name": "@c15t/react",
  "version": "1.8.2",
  "description": "Developer-first CMP for React: cookie banner, consent manager, preferences centre. GDPR ready with minimal setup and rich customization",
  "keywords": [
    "react",
    "consent",
    "privacy",
    "gdpr",
    "ccpa",
    "lgpd",
    "headless",
    "typescript",
    "cookie-banner",
    "consent-management-platform",
    "cmp",
    "consent-banner",
    "user-consent",
    "privacy-compliance",
    "web-privacy"
  ],
  "homepage": "https://c15t.com/docs/frameworks/react/quickstart",
  "repository": {
    "type": "git",
    "url": "https://github.com/c15t/c15t.git",
    "directory": "packages/react"
  },
  "license": "GPL-3.0-only",
  "sideEffects": false,
  "type": "module",
  "exports": {
    "./headless": {
      "types": "./dist/headless.d.ts",
      "import": "./dist/headless.js",
      "require": "./dist/headless.cjs"
    },
    "./consent-manager-dialog": {
      "types": "./dist/components/consent-manager-dialog/index.d.ts",
      "import": "./dist/components/consent-manager-dialog/index.js",
      "require": "./dist/components/consent-manager-dialog/index.cjs"
    },
    "./consent-manager-widget": {
      "types": "./dist/components/consent-manager-widget/index.d.ts",
      "import": "./dist/components/consent-manager-widget/index.js",
      "require": "./dist/components/consent-manager-widget/index.cjs"
    },
    "./cookie-banner": {
      "types": "./dist/components/cookie-banner/index.d.ts",
      "import": "./dist/components/cookie-banner/index.js",
      "require": "./dist/components/cookie-banner/index.cjs"
    },
    "./hooks": {
      "types": "./dist/hooks/index.d.ts",
      "import": "./dist/hooks/index.js",
      "require": "./dist/hooks/index.cjs"
    },
    "./context": {
      "types": "./dist/context/index.d.ts",
      "import": "./dist/context/index.js",
      "require": "./dist/context/index.cjs"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.js",
      "require": "./dist/types/index.cjs"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.js",
      "require": "./dist/utils/index.cjs"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "dependencies": {
    "@radix-ui/react-accordion": "1.2.4",
    "@radix-ui/react-slot": "1.2.0",
    "@radix-ui/react-switch": "1.1.4",
    "clsx": "2.1.1",
    "zustand": "^5.0.3",
    "c15t": "1.8.2"
  },
  "devDependencies": {
    "genversion": "^3.2.0",
    "@c15t/backend": "1.8.0",
    "@c15t/typescript-config": "0.0.1-beta.1",
    "@c15t/vitest-config": "1.0.0"
  },
  "peerDependencies": {
    "react": "^19.0.0 || ^19.0.0-rc || ^18.0.0 || ^17.0.0 || ^16.8.0",
    "react-dom": "^19.0.0 || ^19.0.0-rc || ^18.0.0 || ^17.0.0 || ^16.8.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prebuild": "genversion --esm --semi src/version.ts",
    "build": "pnpm prebuild && rslib build",
    "check-types": "pnpm prebuild && tsc --noEmit",
    "dev": "pnpm prebuild && rslib build --watch",
    "fmt": "pnpm biome format --write . && pnpm biome check --formatter-enabled=false --linter-enabled=false --write",
    "lint": "pnpm biome lint ./src",
    "test": "pnpm prebuild && vitest run",
    "test:watch": "pnpm prebuild && vitest"
  }
}